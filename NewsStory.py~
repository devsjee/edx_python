class NewsStory:
	def __init__(self,guid,title,subject,summary,link):
		self.guid = guid
		self.title = title
		self.subject = subject
		self.summary = summary
		self.link = link

	def getGuid(self):
		return guid
	
	def getTitle(self):
		return title
	
	def getSubject(self):
		return subject

	def getSummary(self):
		return summary

	def getLink(self):
		return link
